        {
          "id": "situation-logement",
          "title": "Êtes-vous :",
          "description": "Sélectionnez votre situation actuelle",
          "type": "radio",
          "choices": [
            {
              "id": "locataire",
              "title": "Locataire (figurant sur le bail, en foyer ou en résidence)"
